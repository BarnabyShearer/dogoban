#!/usr/bin/env python3

import fileinput
import sys
import termios
import tty


def main() -> str:
    level = []
    for height, line in enumerate(fileinput.input()):
        level.extend([char for char in line])
    movement = {"w": -len(line), "d": 1, "s": len(line), "a": -1}
    old = termios.tcgetattr(sys.stdin)
    tty.setcbreak(sys.stdin)
    print("\n" + "".join(level))
    while "ğŸ¯" in level:
        key = sys.stdin.read(1)
        if key in movement:
            for i, tile in enumerate(level):
                if tile == "ğŸ•":
                    if level[i + movement[key]] not in "â¬›ğŸ¯":
                        if level[i + movement[key]] != "ã€€":
                            if level[i + movement[key] * 2] == "ã€€":
                                level[i + movement[key] * 2] = level[i + movement[key]]
                            elif level[i + movement[key] * 2] == "ğŸ¯":
                                level[i + movement[key] * 2] = "ã€€"
                            else:
                                break
                        level[i] = "ã€€"
                        level[i + movement[key]] = "ğŸ•"
                    break
            for _ in range(height + 2):
                sys.stdout.write("\033[F")
            print("".join(level))
    termios.tcsetattr(sys.stdin.fileno(), termios.TCSADRAIN, old)
    print("Well done!")


if __name__ == "__main__":
    main()
